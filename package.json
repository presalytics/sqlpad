{
  "name": "sqlpad-project",
  "version": "5.8.2",
  "private": true,
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "fixlint": "npm run fixlint --prefix client && npm run fixlint --prefix server && prettier --write \"**/*.js\"",
    "lint": "npm run lint --prefix client && npm run lint --prefix server && prettier --check \"**/*.js\""
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.11.0",
    "node-fetch": "^2.6.1"
  }
}
